generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url = env("POSTGRES_PRISMA_URL") 
  directUrl = env("POSTGRES_URL_NON_POOLING") 
  relationMode = "prisma"
}

model TranslatedEntry {
  id  String  @default(uuid()) @id

  kind  String
  original  String @unique
  translated  String @unique
  synonyms  String[]
  antonyms  String[]
  example   String[]
  definition  String
  hint  String
}
